					<ATTRIBUTE_DATA>ENTD0370 - Policy and procedures document not created for proper data handling.</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-56070r1_rule</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
